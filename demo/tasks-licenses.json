		"imageDirectory": "licenses/output/{start-date}/images",
		"outputDirectory": "licenses/datasets",
		"fileName": "dataset-{start-date}-jsoneditor.json"
	},
	{
		"type": "dataset",
		"enabled": true,
		"annotationType": "Cropping",
