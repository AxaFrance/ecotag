{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "",
  "Cors": {
    "origins": "#{Dotnet:CorsOrigins}#"
  },
  "SpaStaticFiles": false,
  "Oidc": {
    "RequireHttpsMetadata": false,
    "Authority": "#{Dotnet:Oidc:Authority}#",
    "ProxyUrl": ""
  },
  "OidcUser": {
    "RequireAudience": "#{Dotnet:OidcUser:RequireAudience}#",
    "RequireScopes": ["#{Dotnet:OidcUser:RequireScope}#"]
  }
}
