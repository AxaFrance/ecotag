{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "",
  "Cors": {
    "origins": "#{Api:CorsOrigins}#"
  },
  "SpaStaticFiles": false,
