{
  "apiUrl": "#{Spa:ApiUrl}#",
  "baseUrl": "#{Spa:BaseUrl}#",
  "oidc": {
    "isEnabled": false,
    "configuration": {
      "client_id": "#{Spa:Oidc:ClientId}#",
      "redirect_uri": "#{Spa:Oidc:RedirectUri}#",
      "post_logout_redirect_uri": "#{Spa:Oidc:PostLogoutUri}#",
      "scope": "#{Spa:Oidc:Scope}#",
      "authority": "#{Spa:Oidc:Authority}#",
      "service_worker_relative_url":"/OidcServiceWorker.js",
      "service_worker_only": true
    }
  },
  "appInsights": {
    "instrumentationKey": "#{APPINSIGHTS_INSTRUMENTATIONKEY}#",
    "logLevel": "#{APPINSIGHTS_LOG_LEVEL}#",
    "active": #{ACTIVE_APPINSIGHTS}#
  }
}
