{
  "apiUrl": "#{apiUrl}#",
  "baseUrl": "/",
  "oidc": {
    "isEnabled": false,
    "configuration": {
      "client_id": "#{Oidc:ClientId}#",
      "redirect_uri": "#{Oidc:RedirectUri}#",
      "response_type": "code",
      "post_logout_redirect_uri": "#{Oidc:PostLogoutRedirectUri}#",
      "scope": "openid profile email api offline_access",
      "authority": "#{Oidc:Authority}#",
      "silent_redirect_uri": "#{Oidc:SilentRedirectUri}#",
      "automaticSilentRenew": true,
      "loadUserInfo": true,
      "triggerAuthFlow": true
    }
  },
  "appInsights": {
    "instrumentationKey": "#{APPINSIGHTS_INSTRUMENTATIONKEY}#",
    "logLevel": "#{APPINSIGHTS_LOG_LEVEL}#",
    "active": #{ACTIVE_APPINSIGHTS}#,
    "trackTrace": #{ACTIVE_APPINSIGHTS_TRACK_TRACE}#,
    "trackEvent": #{ACTIVE_APPINSIGHTS_TRACK_EVENT}#
 }
}
