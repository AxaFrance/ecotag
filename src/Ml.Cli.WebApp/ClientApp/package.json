{
  "name": "ecotag",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@axa-fr/react-oidc": "5.13.0-alpha0",
    "@axa-fr/react-toolkit-all": "1.4.1",
    "@microsoft/applicationinsights-web": "2.8.2",
    "@monaco-editor/react": "4.2.1",
    "classnames": "2.3.1",
    "cuid": "2.1.8",
    "date-fns": "2.28.0",
    "diff-match-patch": "1.0.5",
    "env-cmd": "10.1.0",
    "glob": "7.1.7",
    "js-levenshtein": "1.1.6",
    "konva": "8.3.2",
    "monaco-editor": "0.25.2",
    "mw.validation": "1.0.15",
    "plotty": "0.4.7",
    "postal-mime": "1.0.12",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-hotkeys": "2.0.0",
    "react-intersection-observer": "9.1.0",
    "react-json-pretty": "2.2.0",
    "react-konva": "17.0.2-5",
    "react-on-screen": "2.1.1",
    "react-query": "3.18.1",
    "react-router": "5.2.1",
    "react-router-dom": "5.3.0",
    "react-toastify": "8.0.3",
    "sanitize-html": "2.7.0",
    "sass": "1.45.2",
    "tesseract.js": "2.1.5",
    "use-image": "1.0.7"
  },
  "scripts": {
    "copy": "copyfiles -f ./node_modules/@axa-fr/react-oidc/dist/OidcServiceWorker.js ./public && copyfiles -f -s ./node_modules/@axa-fr/react-oidc/dist/OidcTrustedDomains.js ./public",
    "start:local": "env-cmd -f .env.local react-scripts start",
    "start:server": "env-cmd -f .env.server react-scripts start",
    "build:local": "env-cmd -f .env.local react-scripts build",
    "build:server": "npm run copy && env-cmd -f .env.server react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "postinstall": "npm rebuild canvas --update-binary",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "cp -n ./public/environment.dev.json ./public/environment.dev-custom.json && npm run copy"
  },
  "homepage": ".",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "6.4.22",
    "@storybook/addon-essentials": "6.4.22",
    "@storybook/addon-links": "6.4.22",
    "@storybook/builder-webpack5": "6.4.22",
    "@storybook/manager-webpack5": "6.4.22",
    "@storybook/node-logger": "6.4.22",
    "@storybook/preset-create-react-app": "4.1.0",
    "@storybook/react": "6.4.22",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "copyfiles": "2.4.1",
    "jest-watch-typeahead": "0.6.5",
    "prop-types": "15.8.0",
    "react-scripts": " 5.0.1",
    "typescript": "4.6.4",
    "webpack": "5.72.0"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
